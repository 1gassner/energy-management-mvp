# Production Environment Configuration
NODE_ENV=production
PORT=8001
HOST=0.0.0.0

# Database Configuration
SUPABASE_URL=your-supabase-url
SUPABASE_ANON_KEY=your-supabase-anon-key
SUPABASE_SERVICE_ROLE_KEY=your-supabase-service-role-key

# JWT Configuration
JWT_SECRET=your-strong-jwt-secret-key-here

# Frontend Origins (Production URLs)
ALLOWED_ORIGINS=https://flowmind.yourdomain.com,https://citypulse.yourdomain.com,https://quantum.yourdomain.com,https://velocity.yourdomain.com,https://localhost:3000

# Feature Flags
AI_MODELS_ENABLED=true
CITYPULSE_ENABLED=true
QUANTUM_SWARM_ENABLED=true
VELOCITY_SWARM_ENABLED=true
WEBSOCKET_ENABLED=true

# Rate Limiting
RATE_LIMITING_ENABLED=true

# Logging Configuration
LOG_LEVEL=info

# SSL Configuration
SSL_KEY_PATH=/path/to/ssl/private.key
SSL_CERT_PATH=/path/to/ssl/certificate.crt

# Redis Configuration (for session management and caching)
REDIS_HOST=localhost
REDIS_PORT=6379
REDIS_PASSWORD=your-redis-password

# API Keys (for AI services)
OPENAI_API_KEY=your-openai-api-key
ANTHROPIC_API_KEY=your-anthropic-api-key
GROQ_API_KEY=your-groq-api-key

# Email Configuration (for notifications)
SMTP_HOST=smtp.yourdomain.com
SMTP_PORT=587
SMTP_USER=noreply@yourdomain.com
SMTP_PASS=your-smtp-password

# Monitoring & Analytics
SENTRY_DSN=your-sentry-dsn
ANALYTICS_ENABLED=true

# File Upload Configuration
MAX_FILE_SIZE=10485760
UPLOAD_PATH=/var/uploads

# Security Configuration
CORS_MAX_AGE=86400
SESSION_SECRET=your-session-secret

# Application URLs
FRONTEND_BASE_URL=https://yourdomain.com
API_BASE_URL=https://api.yourdomain.com

# Backup Configuration
BACKUP_ENABLED=true
BACKUP_INTERVAL=24